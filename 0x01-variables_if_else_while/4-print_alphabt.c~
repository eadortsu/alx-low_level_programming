#include <stdio.h>

/**
 * main -  Prints all the alphabets in lowercase then in upercase.
 *
 * Return: Always 0 (Success)
 */
int main(void)
{
int i;
for (i = 97; i < 101; i++)
{
  putchar(i);
}
for (i = 102 ; i < 113; i++)
{
putchar(i);
}
for (i = 114; i < 123; i++)
{
  putchar(i)
  }
return (0);
}
